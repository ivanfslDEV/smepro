# Environment variables declared in this file are automatically made available to Prisma.
# See the documentation for more detail: https://pris.ly/d/prisma-schema#accessing-environment-variables-from-the-schema

# Prisma database connection
DATABASE_URL="postgresql://USER:PASSWORD@HOST/DATABASE?sslmode=require&channel_binding=require"

# Auth secret (generated by npx auth or similar)
AUTH_SECRET="your_auth_secret_here"

# OAuth Providers
AUTH_GITHUB_ID="your_github_client_id"
AUTH_GITHUB_SECRET="your_github_client_secret"
AUTH_GOOGLE_ID="your_google_client_id"
AUTH_GOOGLE_SECRET="your_google_client_secret"

# App URL
NEXT_PUBLIC_URL="http://localhost:3000/"

# Stripe Configuration
NEXT_PUBLIC_STRIPE_PUBLIC_KEY="your_stripe_public_key"
STRIPE_SECRET_KEY="your_stripe_secret_key"
STRIPE_SECRET_WEBHOOK_KEY="your_stripe_webhook_secret"

STRIPE_PLAN_BASIC="your_stripe_basic_plan_id"
STRIPE_PLAN_PROFISSIONAL="your_stripe_profissional_plan_id"
STRIPE_SUCCESS_URL="http://localhost:3000/dashboard/plans"
STRIPE_CANCEL_URL="http://localhost:3000/dashboard/plans"

# Cloudinary Configuration
CLOUDINARY_NAME="your_cloudinary_name"
CLOUDINARY_KEY="your_cloudinary_api_key"
CLOUDINARY_SECRET="your_cloudinary_api_secret"
